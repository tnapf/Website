<div class="item">
    <i class="large teal cloud upload alternate middle aligned icon"></i>
    <div class="content">
        {{ render('parts.events.meta', { event: event })|raw }}
        <div class="header">
            <a target="_blank" href="{{ event.payload.pull_request.html_url }}">
                {{event.payload.pull_request.title }}
            </a> {{ event.payload.pull_request.merged ? 'merged' : 'closed' }} in
            <a href="https://github.com/{{ event.repo.name }}" target="_blank">{{ event.repo.name }}</a>
        </div>
        {{ render('parts.events.time', { event: event })|raw }}
    </div>
</div>